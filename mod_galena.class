package net.minecraft.src;
 
public class mod_galena extends BaseMod{
 
    public static Block oreGalena;
    public int blockID;
    public String fileToOverride;
    public String textureFilePath;
 
    public mod_galena()
    {
    }
    public void load()
    {
    	blockID = ;
    	fileToOverride = "/terrain.png";
    	textureFilePath = "/block/galena.png";
　　　　
    	oreGalena = new BlockGalena(blockID, ModLoader.addOverride(fileToOverride, textureFilePath));
　　　　
    	oreGalena.setHardness(3F);
　　　　
    	oreGalena.setBlockName("oreGalena");
　　　　
    	oreGalena.setResistance(5F);
　　　　
    	oreGalena.setStepSound(Block.soundStoneFootstep);
　　　　
    	oreGalena.setLightValue(1.0F);
　　　　
    	ModLoader.registerBlock(oreGalena);
　　　　
        ModLoader.addName(oreGalena, "Galena Ore");
    }
    public String getVersion()
    {
            return "1.0.0";
    }
}
